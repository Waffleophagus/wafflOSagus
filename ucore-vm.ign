      {
        "name": "rebase-to-wafflosagus.service",
        "enabled": true,
        "contents": "[Unit]\nDescription=Rebase to wafflosagus image\nConditionFirstBoot=yes\nAfter=network-online.target\nWants=network-online.target\n\n[Service]\nType=oneshot\nExecStart=/usr/bin/rpm-ostree rebase --bypass-driver ostree-image-signed:docker://ghcr.io/waffleophagus/wafflosagus-ucore:latest\nExecStart=/usr/bin/systemctl reboot\n\n[Install]\nWantedBy=multi-user.target\n"
      },
      {
        "name": "install-brew-clawdbot.service",
        "enabled": true,
        "contents": "[Unit]\nDescription=Install Homebrew for clawdbot\nAfter=network-online.target\nWants=network-online.target\nConditionPathExists=!/home/linuxbrew/.linuxbrew/bin/brew\n\n[Service]\nType=oneshot\nUser=clawdbot\nGroup=clawdbot\nEnvironment=HOME=/var/home/clawdbot\nEnvironment=USER=clawdbot\nEnvironment=NONINTERACTIVE=1\nExecStart=/usr/bin/install-brew-user.sh\nTimeoutStartSec=600\nStandardOutput=journal\nStandardError=journal\n\n[Install]\nWantedBy=multi-user.target\n"
      }
    ]
